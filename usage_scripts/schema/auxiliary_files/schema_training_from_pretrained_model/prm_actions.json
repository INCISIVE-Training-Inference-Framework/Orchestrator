{
  "actions": [
    {
      "name": "update_to_running",
      "update_status_url": "http://{{inputs.parameters.platform_orchestratorApiHostname}}/api/executions/{{inputs.parameters.execution_id}}/update_to_running/"
    },
    {
      "name": "create_directory",
      "directory_path": "{{inputs.parameters.aiEngine_plaformVarsInputAIElements}}"
    },
    {
      "name": "create_directory",
      "directory_path": "{{inputs.parameters.aiEngine_plaformVarsInputModel}}"
    },
    {
      "name": "create_directory",
      "directory_path": "{{inputs.parameters.aiEngine_plaformVarsOutputElements}}"
    },
    {
      "name": "create_directory",
      "directory_path": "{{inputs.parameters.aiEngine_plaformVarsOutputModel}}"
    },
    {
      "name": "download_user_vars",
      "user_vars_url": "http://{{inputs.parameters.platform_orchestratorApiHostname}}/api/executions/{{inputs.parameters.execution_id}}/download_user_vars/?descriptor=main-ai-engine",
      "output_path": "{{inputs.parameters.aiEngine_plaformVarsInputUserVars}}"
    },
    {
      "name": "download_ai_model",
      "ai_model_url": "http://{{inputs.parameters.platform_maasApiHostname}}/api/ai_models/{{inputs.parameters.execution_main-ai-engine-ai-model}}/download_contents/",
      "output_path": "{{inputs.parameters.aiEngine_plaformVarsInputModel}}"
    },
    {
      "name": "run_ai_engine",
      "use_case": "training_from_pretrained_model",
      "max_iteration_time": 1200,
      "max_initialization_time": 600,
      "client_host": "{{inputs.parameters.aiEngine_plaformVarsApiHost}}",
      "server_host": "{{inputs.parameters.platform_processorResourceManagerApiHost}}",
      "ping_url": "{{inputs.parameters.aiEngine_plaformVarsApiPingUrl}}",
      "run_url": "{{inputs.parameters.aiEngine_plaformVarsApiRunUrl}}",
      "callback_url": "{{inputs.parameters.platform_processorResourceManagerCallbackUrl}}"
    },
    {
      "name": "update_to_succeeded",
      "update_status_url": "http://{{inputs.parameters.platform_orchestratorApiHostname}}/api/executions/{{inputs.parameters.execution_id}}/update_to_succeeded/",
      "upload_ai_model": true,
      "upload_evaluation_metrics": true,
      "upload_generic_file": false,

      "ai_model_upload_url": "http://{{inputs.parameters.platform_maasApiHostname}}/api/ai_models/update_or_create/",
      "ai_model_delete_url": "http://{{inputs.parameters.platform_maasApiHostname}}/api/ai_models/",
      "ai_model_upload_path": "{{inputs.parameters.aiEngine_plaformVarsOutputModel}}",
      "ai_model_user_vars_path": "{{inputs.parameters.aiEngine_plaformVarsInputUserVars}}",
      "ai_model_upload_metadata": {
        "ai_engine_version": {{inputs.parameters.execution_main-ai-engine-version}},
        "name": "{{inputs.parameters.execution_outputAIModelName}}",
        "data_partners_patients": {{inputs.parameters.execution_dataPartnerPatients}},
        "description": "{{inputs.parameters.execution_outputAIModelDescription}}",
        "merge_type": "{{inputs.parameters.execution_outputAIModelMergeType}}",
        "parent_ai_model": {{inputs.parameters.execution_main-ai-engine-ai-model}}
      },

      "evaluation_metrics_upload_url": "http://{{inputs.parameters.platform_maasApiHostname}}/api/evaluation_metrics/update_or_create/",
      "evaluation_metrics_delete_url": "http://{{inputs.parameters.platform_maasApiHostname}}/api/evaluation_metrics/",
      "evaluation_metrics_upload_path": "{{inputs.parameters.aiEngine_plaformVarsOutputEvaluationMetrics}}",
      "evaluation_metrics_upload_metadata": {
        "data_partners_patients": {{inputs.parameters.execution_dataPartnerPatients}}
      }
    }    
  ]
}
